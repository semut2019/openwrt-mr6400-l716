config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/10300000.wmac'
	option channel '6'
	option band '2g'
	option htmode 'HT20'
	option country 'US'
	option disabled '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'OpenWrt-MT7628'
	option encryption 'none'
	option network 'lan'
