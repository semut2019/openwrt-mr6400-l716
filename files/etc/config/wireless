config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/10300000.wmac'
	option htmode 'HT20'
	option channel '11'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'OpenWrt-L716'
	option encryption 'psk2'
	option key '12871287'
	option network 'lan'
